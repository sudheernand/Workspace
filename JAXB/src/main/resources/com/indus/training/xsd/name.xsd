<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://name.name.xsd.training.indus.com/"
	elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema" 
	xmlns:nm="http://name.name.xsd.training.indus.com/">


	<complexType name="Name">
		<sequence>
			<element name="firstName" type="string" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="middleName" type="string" maxOccurs="1"
				minOccurs="0">
			</element>
			<element name="lastName" type="string" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="maidenName" type="string" maxOccurs="1"
				minOccurs="0"></element>
		</sequence>
	</complexType>

	<element name="name" type="nm:Name" ></element>

	<element name="name2" type="nm:Name"></element>
</schema>