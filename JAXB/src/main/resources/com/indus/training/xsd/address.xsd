<?xml version="1.0" encoding="UTF-8"?>
<schema targetNamespace="http://address.xsd.training.indus.com/"
	elementFormDefault="qualified" xmlns="http://www.w3.org/2001/XMLSchema" xmlns:add="http://address.xsd.training.indus.com/">

	<complexType name="Address">
		<sequence>
			<element name="houseNumber" type="string" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="streetNumber" type="string" maxOccurs="1"
				minOccurs="1">
			</element>
			<element name="city" type="string" maxOccurs="1" minOccurs="1">
			</element>
			<element name="phoneNumber" type="string" maxOccurs="1"
				minOccurs="0"></element>
		</sequence>
	</complexType>

	<element name="address" type="add:Address"></element>
</schema>