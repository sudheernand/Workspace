<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
	<display-name>Archetype Created Web Application</display-name>
	<servlet>
		<servlet-name>WelcomeServlet</servlet-name>

		<description></description>
		<servlet-class>com.indus.training.servlet.WelcomeServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>WebPageServlet</servlet-name>
		<display-name>WebPageServlet</display-name>
		<description></description>
		<servlet-class>com.indus.training.servlet.WebPageServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>WelcomeServlet</servlet-name>
		<url-pattern>/WelcomeServlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>WebPageServlet</servlet-name>
		<url-pattern>/WebPageServlet</url-pattern>
	</servlet-mapping>
</web-app>
